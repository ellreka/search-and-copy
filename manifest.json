{
  "manifest_version": 2,
  "name": "search and copy",
  "description": "検索とコピーを同時に行います",
  "version": "0.1.1",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "icons":{
    "128": "icon128.png"
  },

  "permissions": [
    "tabs",
    "contextMenus",
    "clipboardRead"
  ]

}

